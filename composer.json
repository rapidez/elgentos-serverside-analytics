{
    "name": "rapidez/elgentos-serverside-analytics",
    "description": "Rapidez Elgentos Serverside Analytics",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:rapidez/core.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0",
        "rapidez/core": "dev-master",
        "irazasyed/laravel-gamp": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\ElgentosServersideAnalytics\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\ElgentosServersideAnalytics\\ElgentosServersideAnalyticsServiceProvider"
            ]
        }
    }
}
